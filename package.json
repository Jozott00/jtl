{
  "name": "jtl",
  "displayName": "JTL",
  "description": "JTL Language VSCode support",
  "version": "0.0.1",
  "publisher": "johanneszottele",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "path": "./syntaxes/highlight-jtl-comment.json",
        "scopeName": "inline-jtl.injection",
        "injectTo": [
          "source.cpp"
        ],
        "embeddedLanguages": {
            "meta.embedded.jtl": "jtl"
        }
    },
    {
      "scopeName": "source.jtl",
      "path": "./syntaxes/jtl.tmGrammar.json"
    }
    ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  }
}
